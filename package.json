{
  "name": "electron-serialport",
  "version": "1.0.1",
  "description": "A minimal Electron application with node serialport",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "install": "electron-rebuild",
    "packageWin": "electron-packager ./ Assistant --platform=win32 --arch=all --icon=hosts.ico --out=./dist --asar --app-version=2.0.1",
    "packageLinux": "electron-packager ./ Assistant --platform=linux --arch=x64 --out=./dist --asar --app-version=2.0.1",
    "packageDarwin": "electron-packager ./ Assistant --platform=darwin --arch=x64 --icon=hosts.icns --out=./dist --asar --app-version=2.0.1"
  },
  "keywords": [
    "Electron",
    "serialport",
    "demo"
  ],
  "devDependencies": {
    "electron": "^1.4.1",
    "electron-packager": "^8.6.0",
    "electron-rebuild": "^1.4.0"
  },
  "dependencies": {
    "serialport": "^4.0.7"
  }
}
